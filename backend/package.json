{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn ./src/bin/server.ts",
    "dev": "nodemon --exec ts-node ./src/bin/server.ts",
    "build": "npm run clean && npm run build-ts && npm run copy-static-assets",
    "clean": "del-cli --force ./dist",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "build:watch": "tsc --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.1",
    "@types/shelljs": "^0.8.8",
    "del-cli": "^3.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63"
  }
}
